{
  "stan": {

  	 "name": "Osmium",

	 "model":	{
	 	"file": "./OsmSTAN.stan",
		"cache": "./cache"
		},

	 "data": {
	 	"type": "mc",
		"files": [
		    {
			"name": "./osmium_data.data", 
			"format":"R"
		    }
		 ],
		"parameters": [
		      {
		        "sin2theta_s" : 0.5,
			"delta_m2" : 1.0
		      }
		    ]
	},

	"run": {
	       "algorithim": "NUTS",
	       "iter": 2500,
	       "chain": 8},

	"plot": [
		{"variable": "theFlux"},
		{"variable": "theSpectrum"},
		{"variable": "theCrossSection"},
		{"variable": "theRate"},
		{"variable": "neutrino_energy"},
		{"variable": "kinetic_energy"},
		{"variable": "radius"},
		{"variable": "time"},
		{"variable": "lp__"}
		],

	"sample": "./results/test_generator.out"

   }

}
